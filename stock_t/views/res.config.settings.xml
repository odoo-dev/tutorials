<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form_inherit_stock_t" model="ir.ui.view">
        <field name="name">model.name.view.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//block[@name='shipping_setting_container']//setting[1]" position="after">
                <setting id="stock_transport" company_dependent="1" help="Transport Management: organize packs in your fleet, or carriers.">
                    <field name="module_stock_transport"/>
                </setting>
            </xpath>
        </field>
    </record>
</odoo>
