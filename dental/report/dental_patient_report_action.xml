<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="dental_patient_report_action" model="ir.actions.report">
        <field name="name">Dental Patient</field>
        <field name="model">dental.patient</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">dental.dental_patient_report_view</field>
        <field name="report_file">dental.dental_patient_report_view</field>
        <field name="print_report_name">'Billing Report - %s' % (object.name or
            'Attendee').replace('/','') </field>
        <field name="binding_model_id" ref="model_dental_patient" />
        <field name="binding_type">report</field>
    </record>

</odoo>