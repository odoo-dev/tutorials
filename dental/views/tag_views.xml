<odoo>
    <record id="detal_tags_action" model="ir.actions.act_window">
        <field name="name">Tags</field>
        <field name="res_model">dental.tags</field>
        <field name="view_mode">tree</field>
        <field name="context">{'search_default_state': True, 'search_default_current': True}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a new Property product
            </p>
        </field>
    </record>

     <record id='detal_tags_tree' model='ir.ui.view'>
        <field name="name">Tags Tree</field>
        <field name="model">dental.tags</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name='name'/>
            </tree>
        </field>
    </record>
</odoo>