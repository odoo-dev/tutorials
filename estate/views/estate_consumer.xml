<?xml version="1.0" encoding="utf-8"?>
<odoo>

        
    <record id="action_estate_consumer_form" model="ir.actions.act_window">
        <field name="name">Consumer Form</field>
        <field name="res_model">estate.consumer</field>
        <field name="view_mode">form</field>
        <field name="view_id"/>
         <!-- ref="estate.view_estate_consumer_form"
         />  -->
        <field name="target">new</field>
    </record>

    <record id = "view_estate_consumer_form" model="ir.ui.view">
        <field name="name">estate.consumer.form</field>
        <field name="model">estate.consumer</field>
        <field name="arch" type="xml">
            <form string="Consumer">
            <header>
                <button  name="action_anonymous" type="object" string="Anonymous User"/>
                <button  name="action_string" type="object" string="Action String"/>
                <button  name="action_number" type="object" string="Action Number"/>
                <button  name="action_dict" type="object" string="Action Dictionary" />
                <!-- <button name="edit" type="edit" icon="fa-edit" title="Edit"/>
                <button type="delete" name="delete" title="Delete Action" string="Delete"/> -->
            </header>
            
                <group>
                    <field name="name" />
                    <field name="email"  />
                    <field name="contact" />
                </group>
            </form>
        </field>
    </record>
</odoo>
