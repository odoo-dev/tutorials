<odoo>
    <template id="res_users_report_template">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <!-- Iterate over the 'docs' context variable -->
                <t t-foreach="docs" t-as="user">
                    <div class="user-detail">
                        <!-- Display User Name -->
                        <h4>User Name: <span t-field="user.name" /></h4>

                        <!-- Static Text for Testing -->
                        <h1>This is a test to check if the sub-template is called correctly.</h1>

                        <!-- Add more fields if needed -->
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
