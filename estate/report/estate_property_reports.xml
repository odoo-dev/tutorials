<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="estate_property_report" model="ir.actions.report">
        <field name="name">Print property</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="report_file">estate.report_property_offers</field>
        <field name="print_report_name">'Estate - %s' % (object.name or 'Properties').replace('/','')</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>
    <record id="estate_salesman_property_report" model="ir.actions.report">
        <field name="name">Print Properties</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">estate.report_salesman_property_offers</field>
        <field name="report_file">estate.report_salesman_property_offers</field>
        <field name="print_report_name">'Estate - %s' % (object.name or 'Properties').replace('/','')</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>
