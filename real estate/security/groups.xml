<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.module.category" id='module_category_real_estate'>
        <field name='name'>Real Estate</field>
        <field name="description">Category for real estate</field>
    </record>
    <record id="group_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id" ref="module_category_real_estate"></field>
    </record>
    <record id="group_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_real_estate"></field>
        <field name="implied_ids" eval="[(4,ref('group_admin'))]"></field>
    </record>
</odoo>